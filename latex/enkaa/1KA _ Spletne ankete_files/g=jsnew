return deferred;}
function mousedown(e){if(!$.mapster.hasCanvas){this.blur();}
e.preventDefault();}
function mouseover(me,e){var arData=me.getAllDataForArea(this),ar=arData.length?arData[0]:null;if(!ar||ar.isNotRendered()||ar.owner.currentAction){return;}
