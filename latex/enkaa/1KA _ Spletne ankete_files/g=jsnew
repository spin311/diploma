{$('#fade').fadeTo('slow',1);$('#fullscreen').fadeIn('slow');$("#fullscreen").load('ajax.php?t=variableProfile&a=displayProfile',{anketa:srv_meta_anketa_id,podstran:srv_meta_podstran,pid:pid});}
function displayVariableProfile()
