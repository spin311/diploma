else{$('#alert_show_96_'+sprid).attr("checked",false);$('#alert_show_97_'+sprid).attr("checked",false);$('#alert_show_98_'+sprid).attr("checked",false);$('#alert_show_99_'+sprid).attr("checked",false);}}
